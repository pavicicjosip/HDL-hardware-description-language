//removes the two most left bit
//e.g. in = 101010 -> out = 1010 

CHIP SixTOfour
{
    IN in[6];
    OUT out[4];

    PARTS:
    Or(a= false, b= in[0], out= out[0]);
    Or(a= false, b= in[1], out= out[1]);
    Or(a= false, b= in[2], out= out[2]);
    Or(a= false, b= in[3], out= out[3]);
}